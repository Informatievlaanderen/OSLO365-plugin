    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Edge",
            "port": 3000,
            "request": "attach",
            "type": "pwa-msedge",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "office-addin",
            "request": "attach",
            "name": "Attach to Office Add-ins",
            "port": 3000,
            "trace": "verbose",
            "url": "http://127.0.0.1:3000//taskpanetwo.html?_host_Info=Word$Win32$16.01$nl-NL$$$$0",
            "webRoot": "${workspaceFolder}",
            "timeout": 45000
          }
    ]
}